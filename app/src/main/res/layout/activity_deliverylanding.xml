        android:layout_width="196dp"
        android:layout_height="71dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="monospace"
        android:text="@string/frcp"
        android:textAlignment="viewStart"
        android:textColor="@color/txc"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.78"
        app:layout_constraintStart_toEndOf="@+id/di2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.293" />

    <TextView
        android:id="@+id/trh"
        android:layout_width="82dp"
        android:layout_height="34dp"
        android:layout_marginEnd="128dp"
        android:fontFamily="@font/roboto_bold"
        android:text="@string/kttu"
        android:textColor="@color/rh"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/trds"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.794"
        app:layout_constraintStart_toEndOf="@+id/di3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/trds"
        android:layout_width="208dp"
        android:layout_height="68dp"
        android:fontFamily="monospace"
        android:text="@string/kttup"
        android:textAlignment="viewStart"
        android:textColor="@color/txc"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.773"
        app:layout_constraintStart_toEndOf="@+id/di3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.558" />

    <TextView
        android:id="@+id/qrh"
        android:layout_width="91dp"
        android:layout_height="31dp"
        android:fontFamily="@font/roboto_bold"
        android:text="@string/hpprs"
        android:textColor="@color/rh"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/qrds"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.314"
        app:layout_constraintStart_toEndOf="@+id/di4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/qrds"
        android:layout_width="193dp"
        android:layout_height="48dp"
        android:layout_marginEnd="12dp"
        android:fontFamily="monospace"
        android:text="@string/hpprsp"
        android:textAlignment="textStart"
        android:textColor="@color/txc"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.75"
